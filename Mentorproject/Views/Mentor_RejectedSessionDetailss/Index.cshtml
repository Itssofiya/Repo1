@model IEnumerable<Mentorproject.Mentor_RejectedSessionDetailss>

@{
    ViewBag.Title = "Index";
}

<h2 style="font-family:Cooper;color:black">Sessions Rejected</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table" style="background-image: url('../../IMGS/blackred.jpg');color:whitesmoke" />
  <tr>
    <th style="text-size-adjust:350%">
        @Html.DisplayNameFor(model => model.DomainId)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.SessionId)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.SessionDate)
    </th>
    <th>
        @Html.DisplayNameFor(model => model.RejectReason)
    </th>
    <th></th>
    </tr>

     @foreach (var item in Model)
   {
     <tr>
         <td>
            @Html.DisplayFor(modelItem => item.DomainId)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.SessionId)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.SessionDate)
         </td>
         <td>
            @Html.DisplayFor(modelItem => item.RejectReason)
         </td>
         <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.MentorId }) |
            @Html.ActionLink("Details", "Details", new { id = item.MentorId }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.MentorId })
         </td>
     </tr>
   }
</table>